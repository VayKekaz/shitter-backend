{
  "name": "shitter-backend",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "main": "./dist/app.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "app": "ts-node src/app.ts",
    "install": "deepkit-type-install",

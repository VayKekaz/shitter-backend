    "@deepkit/bson": "^1.0.1-alpha.72",
    "@deepkit/core": "^1.0.1-alpha.72",
    "@deepkit/core-rxjs": "^1.0.1-alpha.72",
    "@deepkit/event": "^1.0.1-alpha.72",
    "@deepkit/filesystem": "^1.0.1-alpha.108",
    "@deepkit/framework": "^1.0.1-alpha.72",
    "@deepkit/http": "^1.0.1-alpha.72",
    "@deepkit/injector": "^1.0.1-alpha.72",
    "@deepkit/logger": "^1.0.1-alpha.72",
    "@deepkit/orm": "^1.0.1-alpha.108",
    "@deepkit/postgres": "^1.0.1-alpha.108",
    "@deepkit/rpc": "^1.0.1-alpha.72",
    "@deepkit/rpc-tcp": "^1.0.1-alpha.72",
    "@deepkit/sql": "^1.0.1-alpha.72",
    "@deepkit/sqlite": "^1.0.1-alpha.72",
    "@deepkit/stopwatch": "^1.0.1-alpha.72",
    "@deepkit/template": "^1.0.1-alpha.72",
    "@deepkit/type": "^1.0.1-alpha.72",
    "@deepkit/type-compiler": "^1.0.1-alpha.72",
    "@deepkit/workflow": "^1.0.1-alpha.72",
    "@types/jsonwebtoken": "^9.0.5",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^28.1.6",
    "faker": "^5.5.3",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "node",
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "packageManager": "yarn@4.0.2"
}
